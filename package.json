{
  "name": "wikigaialab",
  "version": "0.1.0",
  "private": true,
  "description": "Community-driven problem solving platform",
  "keywords": ["nextjs", "supabase", "ai", "community", "voting"],
  "author": "WikiGaiaLab Team",
  "license": "MIT",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "pnpm --filter @wikigaialab/web dev",
    "build": "pnpm -r build",
    "start": "pnpm --filter @wikigaialab/web start",
    "lint": "pnpm -r lint",
    "lint:fix": "pnpm -r lint --fix",
    "type-check": "pnpm -r type-check",
    "test": "pnpm -r test",
    "test:watch": "pnpm -r test --watch",
    "test:coverage": "pnpm -r test --coverage",
    "test:e2e": "pnpm --filter @wikigaialab/web test:e2e",
    "db:push": "supabase db push",
    "db:reset": "supabase db reset",
    "db:generate": "supabase gen types typescript --local > packages/database/src/types/supabase.ts",
    "db:seed": "tsx scripts/seed.ts",
    "db:migrate": "supabase migration new",
    "db:test": "node scripts/test-db-connection.js",
    "stripe:listen": "stripe listen --forward-to localhost:3000/api/webhooks/stripe",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "deploy:staging": "vercel --target staging",
    "deploy:production": "vercel --prod",
    "clean": "pnpm -r clean",
    "setup": "pnpm install && node scripts/setup.js"
  },
  "dependencies": {
    "openai": "5.10.1",
    "@anthropic-ai/sdk": "0.56.0",
    "resend": "2.0.0"
  },
  "devDependencies": {
    "eslint-config-next": "14.0.3",
    "@types/jest": "29.5.7",
    "@playwright/test": "1.40.0",
    "eslint": "8.53.0",
    "eslint-config-prettier": "9.0.0",
    "@typescript-eslint/eslint-plugin": "6.12.0",
    "@typescript-eslint/parser": "6.12.0",
    "prettier": "3.0.3",
    "prettier-plugin-tailwindcss": "0.5.6",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "@testing-library/react": "14.0.0",
    "@testing-library/jest-dom": "6.1.4",
    "@testing-library/user-event": "14.5.1",
    "tsx": "4.1.1",
    "supabase": "1.100.0",
    "@vercel/analytics": "1.1.1",
    "@sentry/nextjs": "7.77.0",
    "husky": "8.0.3",
    "ts-jest": "29.1.1",
    "typescript": "5.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}